# API key for admin endpoints (required)
ADMIN_API_KEY=

# Default URL for the CLI tool (e.g. http://localhost:3010 or https://phtun.example.com)
TUNNEL_URL=http://localhost:3010

# Port to listen on
PORT=3010

# Environment (development, production, test)
ENV=development

# PostHog hosts (defaults to EU)
POSTHOG_HOST=eu.i.posthog.com
POSTHOG_ASSETS_HOST=eu-assets.i.posthog.com

# Paths to SSL certificate and key files
SSL_CERT_PATH=
SSL_KEY_PATH=
